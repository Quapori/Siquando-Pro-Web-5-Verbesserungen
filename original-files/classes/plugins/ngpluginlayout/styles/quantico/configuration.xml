		<aspectratio>Ratio1by1</aspectratio>
		<showlink>true</showlink>
		<showtitle>true</showtitle>
		<showoverride>false</showoverride>
	</config>
	<config type="NGPluginLayoutConfigPicture" id="eyecatcher"
