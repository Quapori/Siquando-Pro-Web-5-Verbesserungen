#ngparabloghead{$uid}>div { background-color:#{$colorbackground};color: #{$colorforeground}; }
#ngparabloghead{$uid}>div>h2, #ngparabloghead{$uid}>div>h3 { color:#{$colorforeground}; }
#ngparabloghead{$uid}>div a { color:#{$colorforeground};text-decoration:none;font-weight:normal; }
#ngparabloghead{$uid}>div a:hover { color:#{$colorforeground};text-decoration:underline;font-weight:normal; }